{
  "name": "mask-truffle-ai",
  "version": "1.0.0",
  "description": "基于Truffle和React的区块链数据上链系统 - Blockchain Data Query System with Truffle and React",
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "migrate:reset": "truffle migrate --reset",
    "migrate:sepolia": "truffle migrate --network sepolia",
    "migrate:ganache": "truffle migrate --network development --reset",
    "migrate:mainnet": "truffle migrate --network mainnet",
    "test": "truffle test",
    "test:coverage": "truffle run coverage",
    "console": "truffle console",
    "install-all": "npm install && cd client && npm install",
    "clean": "rm -rf build/ && cd client && rm -rf build/"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "truffle",
    "react",
    "web3",
    "smart-contracts",
    "defi",
    "data-storage"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juzhiqiang/mask-truffle-ai.git"
  },
  "bugs": {
    "url": "https://github.com/juzhiqiang/mask-truffle-ai/issues"
  },
  "homepage": "https://github.com/juzhiqiang/mask-truffle-ai#readme",
  "devDependencies": {
    "@ganache/cli": "^0.10.2",
    "@truffle/hdwallet-provider": "^2.1.15",
    "chai": "^4.4.0",
    "dotenv": "^16.4.0",
    "ganache": "^7.9.2",
    "truffle": "^5.11.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "truffle-plugin-verify": "^0.6.7"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
